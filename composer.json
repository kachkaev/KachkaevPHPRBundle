{
    "name": "kachkaev/php-r-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle for integration with php-r library that provides ability to run R scripts from PHP",
    "keywords": ["r", "statistical analysis", "stats", "bridge"],
    "homepage": "https://github.com/kachkaev/PHPRBundle",
    "license": "MIT",
    "authors": [{
        "name": "Alexander Kachkaev",
        "email": "alexander@kachkaev.ru",
        "homepage": "http://en.kachkaev.ru/"
    },
    {
        "name": "Contributors",
        "homepage": "https://github.com/kachkaev/php-/contributors"
    }],
    "require": {
        "php": "~5.3",
        "kachkaev/php-r": "1.0.*@dev",
        "symfony/framework-bundle": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Kachkaev\\PHPRBundle\\": ""
        }
    },
    "target-dir": "Kachkaev/PHPRBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
